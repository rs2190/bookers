<h1>Books</h1>

<h2>New Books</h2>
<!-- form_with部分 -->
<%= form_with model: @book, url:'/books', method: :post do |b| %>
<div>
  <label>Title</label>
  <div>
    <%= b.text_field :title %>
  </div>
</div>
<div>
  <label>Body</label>
  <div>
    <%= b.text_area :body %>
  </div>
</div>
<div>
  <%= b.submit 'Create book' %>
</div>
<% end %>